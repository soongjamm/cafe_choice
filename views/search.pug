extends layouts/main
include mixins/searchedBlock

block content
    .search
        br
        p.display-4.container #{searchingBy}(으)로 검색한 결과입니다.

        each item in cafes
            +searchedBlock({
                id : item.id,
                name : item.name,
                location : item.location,
                imageUrl : item.imageUrl,
                amenities : item.amenities,
                menu : item.menu
            })
        