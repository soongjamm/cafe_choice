extends layouts/main
include mixins/searchedBlock

block content
    .search
        each item in cafes
            +searchedBlock({
                id : item.id,
                name : item.name,
                location : item.location,
                imageUrl : item.imageUrl,
                amenities : item.amenities,
                menu : item.menu
            })
        h2 #{searchingBy}(으)로 검색한 결과 