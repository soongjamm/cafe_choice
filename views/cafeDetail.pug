extends layouts/main
include mixins/cafeInfo
include mixins/cafeBlock
include mixins/reviewBlock
include mixins/map
include mixins/menu

block content
    +cafeInfo({
        id : cafe.id,
        name : cafe.name,
        location : cafe.location,
        imageUrl : cafe.imageUrl, 
        menu : cafe.menu,
        amenities : cafe.amenities
    })  
    
    +map({
        id : cafe.id,
        latlng : cafe.latlng
    })

    +menu({
        id : cafe.id,
        menu : cafe.menu
    })