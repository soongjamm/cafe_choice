extends layouts/main
include mixins/cafeBlock


block content
    .listAll
        .card-columns
            each item in cafes
                +cafeBlock({
                    id : item.id,
                    name : item.name,
                    location : item.location,
                    imageUrl : item.imageUrl,
                    amenities : item.amenities,
                    menu : item.menu,
                    latlng : item.latlng,
                    branch : item.branch,
                    comments : item.comments
                })